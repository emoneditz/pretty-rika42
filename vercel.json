{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "chat.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.js"
        },
        {
            "src": "/chat.html",
            "dest": "chat.html"
        },
        {
            "src": "/(.*)",
            "dest": "index.html"
        }
    ]
}
